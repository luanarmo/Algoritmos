<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="Tabla.TablaController"
          prefHeight="400.0" prefWidth="800.0" alignment="CENTER" hgap="10" vgap="10">
    <padding><Insets bottom="10" top="10"/></padding>
    <HBox GridPane.rowIndex="0" alignment="TOP_CENTER">
        <Text fx:id="titulo" text="Resultados" />
    </HBox>
    <HBox GridPane.rowIndex="1" spacing="5">
        <TableView fx:id="tblResultados"/>
            <VBox spacing="5">
                <Button fx:id="btnAceptar" text="Aceptar" minWidth="40"/>
                <Button fx:id="btnCancelar" text="Cancelar" minWidth="30"/>
            </VBox>
    </HBox>
</GridPane>
